<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>

<BorderPane prefHeight="400.0" prefWidth="767.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CodeEditor.CodeEditor">
   <center>
      <VBox prefHeight="400.0" prefWidth="375.0">
         <children>
            <WebView fx:id="webview" minHeight="-Infinity" minWidth="-Infinity" onKeyReleased="#updateInvalidLocators" prefHeight="196.0" prefWidth="329.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </WebView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="copyCode" mnemonicParsing="false" onAction="#setCode" text="Set Feature Template" />
                  <Button fx:id="createFeature" mnemonicParsing="false" onAction="#createNewFeature" text="Save feature file" />
               </children>
            </HBox>
            <Label fx:id="labeledCode" graphicTextGap="5.0" lineSpacing="1.0" text="Invalid locators" />
            <ComboBox fx:id="locatorComboBox" prefWidth="150.0" />
         </children>
      
      </VBox>
   </center>
   <left>
      <TreeView fx:id="fileTreeView" prefHeight="400.0" prefWidth="122.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Valid Locators Suggestions" />
            <ListView fx:id="predictedLocatorsList" editable="true" prefHeight="400.0" prefWidth="208.0" />
         </children>
      </VBox>
   </right>
</BorderPane>
