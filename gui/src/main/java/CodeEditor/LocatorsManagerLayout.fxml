<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CodeEditor.LocatorsManager">
   <children>
      <TableView fx:id="locatorsTable">
        <columns>
          <TableColumn fx:id="nameColumn" minWidth="300.0" prefWidth="-1.0" text="Name" />
          <TableColumn fx:id="valueColumn" minWidth="300.0" prefWidth="-1.0" text="Value" />
        </columns>
      </TableView>
      <HBox fillHeight="false" spacing="8.0">
         <children>
            <TextField fx:id="nameField" promptText="Name" />
            <TextField fx:id="valueField" promptText="Value" />
            <Button fx:id="btn" mnemonicParsing="false" onAction="#initTable" text="Initiate" />
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addLocator" text="Add Locator" />
            <Button fx:id="searchButton" mnemonicParsing="false" text="Search" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding></HBox>
   </children>
</VBox>
