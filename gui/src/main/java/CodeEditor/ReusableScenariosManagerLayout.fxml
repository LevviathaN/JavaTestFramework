   <children>
      <HBox>
         <children>
            <ListView fx:id="reusablesListView" onMouseClicked="#updateStepsList" />
            <ListView fx:id="scenarioStepsListView" />
         </children>
      </HBox>
      <Button fx:id="initiateButton" mnemonicParsing="false" onAction="#initiateList" text="Button" />
   </children>
</VBox>
