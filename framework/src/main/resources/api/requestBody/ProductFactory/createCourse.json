{
  "operationName": "createCourse",
  "variables": {
    "command": {
      "paperReference": "EC_PAPER_REFERENCE",
      "levelReference": "EC_LEVEL_REFERENCE",
      "sittingReference": "EC_SITTING_REFERENCE",
      "courseTypeReference": "EC_COURSE_TYPE_REFERENCE",
      "regionReference": "EC_REGION_REFERENCE",
      "numberOfSessions": 1,
      "numberOfSteps": 1
    }
  },
  "query": "mutation createCourse($command: CreateCourseCommandInput!) { createCourse(command: $command) { reference __typename }}"
}